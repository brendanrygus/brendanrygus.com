{
  "name": "brendanrygus.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@styled-system/theme-get": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "polished": "^3.5.1",
    "ramda": "^0.27.0",
    "react": "0.0.0-experimental-94c0244ba",
    "react-cache": "^2.0.0-alpha.1",
    "react-dom": "0.0.0-experimental-94c0244ba",
    "react-error-boundary": "^2.3.1",
    "react-helmet-async": "^1.0.6",
    "react-intersection-observer": "^8.27.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "styled-components": "^5.2.0-test.10",
    "styled-system": "^5.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:server": "cd server && webpack-dev-server",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "import/order": [
        "error",
        {
          "pathGroups": [
            {
              "pattern": "~/**",
              "group": "internal"
            }
          ],
          "groups": [
            "builtin",
            "external",
            "internal",
            "parent",
            [
              "index",
              "sibling"
            ]
          ],
          "newlines-between": "always",
          "pathGroupsExcludedImportTypes": [
            "builtin"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-cli/coverage": "2.1.0",
    "@graphql-cli/diff": "2.1.0",
    "@graphql-cli/similar": "2.1.0",
    "@graphql-cli/validate": "2.1.0",
    "@svgr/cli": "^5.4.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql-cli": "4.0.0",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
